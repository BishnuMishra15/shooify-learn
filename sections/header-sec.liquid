<div class="header-wrapper">
  <div class="logo-container">
    <img src="{{ 'comercial.svg' | asset_url }}" alt="Commercial" height="50" width="50" class="main_logo">
  </div>
  <ul class="links">
    {% for link in linklists['main-menu'].links %}
      <li>
        <a href="{{ link.url }}">{{ link.title }}</a>
      </li>
    {% endfor %}

    {% comment %} <img src="{{ 'icon-cart.svg' | asset_url }}" width="400" height="400" alt="Cart Icon"> {% endcomment %}
  </ul>
</div>

<style>
  .header-wrapper {
    display: flex;
    padding: 10px;
  }
  .logo-container {
    flex: 1;
  }
  .main_logo {
    border-radius: 50%;
  }

  .links {
    flex: 4;
    display: flex;
    list-style: none;
    gap: 20px;
  }
</style>
